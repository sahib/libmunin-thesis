digraph {
    // size=4; 
    splines=false;
    node [shape=box, style=filled, fillcolor="#88aaff"];
    rankdir="LR";
    
    subgraph {
        rank=same;
        " †" [fillcolor="#ffaa88"]
        "† " [fillcolor="#ffaa88"]
    }

    subgraph {
        rank=same;
        "{☑, ☒}"
        "{☒, ☑}"
    }
    "Start ➜" [fillcolor="#aaff88"]

    "Start ➜" -> "{☒, ☒}" [label=" {☒, ☒}"]
    "{☒, ☒}" -> "{☑, ☒}" [label="{alternative, ☒}"]
    "{☒, ☒}" -> "{☒, ☑}" [label=" {☒, rock}"]
    "{☒, ☑}" -> "{☑, ☑}" [label=" {rock, alternative}"]
    "{☑, ☒}" -> "† " [label="{alternative, ☒}"]
    "{☑, ☑}" -> " †"
}
