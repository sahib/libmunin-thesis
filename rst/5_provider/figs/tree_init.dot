digraph {
    size=4; 
    node [shape=record, style=filled, fillcolor="#ffffff"];
    splines=false;
    outputorder="edgesfirst";

    subgraph {
        rank=same;
        a [label="<0> Alternative Pop (#0)"];
        b [label="<1> Alternative Rock (#1)"];
        c [label="<2> j-Pop (#2)"];
    }
    subgraph {
        rank=same;
        X1 [style=invisible];
        X2 [style=invisible];
        X3 [style=invisible];
        d [label="<3> Reggae (#3)"];
        e [label="<4> Swedish Alternative Rock (#4)"];
        f [label="<5> Swedish Pop (#5)"];
    }

    d -> X1 [style=invis, dirType="none"];
    e -> X2 [style=invis, dirType="none"];
    f -> X3 [style=invis, dirType="none"];

    a -> X1 [style=invis, dirType="none"];
    b -> X2 [style=invis, dirType="none"];
    c -> X3 [style=invis, dirType="none"];

    "Music (#0)" -> d;
    "Music (#0)" -> e;
    "Music (#0)" -> f;

    "Music (#0)" -> a;
    "Music (#0)" -> b;
    "Music (#0)" -> c;
}
