    size=4; 
    node [shape=record, style=filled, fillcolor="#ffaa88"];
    splines=false;
    outputorder="edgesfirst";

    "Music (#0)" [fillcolor="#ff8888"]

    subgraph {
        rank=same;
        a [label="<0> &nbsp;Alternative Pop (#0)&nbsp;"];
        b [label="<1> &nbsp;Alternative Rock (#1)&nbsp;"];
        c [label="<2> &nbsp;j-Pop (#2)&nbsp;"];
    }
    subgraph {
        rank=same;
        X1 [style=invisible];
        X2 [style=invisible];
        X3 [style=invisible];
        d [label="<3> &nbsp;Reggae (#3)&nbsp;"];
        e [label="<4> &nbsp;Swedish Alternative Rock (#4)&nbsp;"];
        f [label="<5> &nbsp;Swedish Pop (#5)&nbsp;"];
    }

    d -> X1 [style=invis, dirType="none"];
    e -> X2 [style=invis, dirType="none"];
    f -> X3 [style=invis, dirType="none"];

    a -> X1 [style=invis, dirType="none"];
    b -> X2 [style=invis, dirType="none"];
    c -> X3 [style=invis, dirType="none"];

    "Music (#0)" -> d;
    "Music (#0)" -> e;
    "Music (#0)" -> f;

    "Music (#0)" -> a;
    "Music (#0)" -> b;
    "Music (#0)" -> c;
